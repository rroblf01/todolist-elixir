<h1>Show Note</h1>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @note.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @note.description %>
  </li>

  <li>
    <strong>Completed:</strong>
    <%= @note.completed %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.note_path(@conn, :edit, @note) %></span>
<span><%= link "Back", to: Routes.note_path(@conn, :index) %></span>
